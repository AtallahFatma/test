    {% extends 'admin.html.twig' %}

{% block body %}

    <section class="panel">
        <header class="panel-heading">
            <h1>Test33s list</h1>
        </header>
        <div class="panel-body">
            <div class="adv-table editable-table">
                <div class="clearfix">
                                        <a href="{{ path('test33_new') }}">
                        <div class="btn-group">
                            <button type="button" class="btn btn-primary"><a href="{{ path('test33_new') }}">Create a newtest33</a> </button>
                        </div>
                    </a>
                                    </div>
                <br>
                    <table class="display table table-striped table-hover table-bordered">
                        <thead>
                            <tr>
                                <th>Id</th>
                                <th>Nom</th>
                                <th>Pr</th>
                                <th>Fdskfkd</th>
                                <th>Actions</th>
                            </tr>
                        </thead>

                            <tfoot>
                        {% for test33 in test33s %}
                            <tr>
                                <td><a href="{{ path('test33_show', { 'id': test33.id }) }}">{{ test33.id }}</a></td>
                                <td>{{ test33.nom }}</td>
                                <td>{{ test33.pr }}</td>
                                <td>{{ test33.fdskfkd }}</td>
                                <td>
                                    <ul>
                                        <li>
                                            <a href="{{ path('test33_show', { 'id': test33.id }) }}">show</a>
                                        </li>
                                        <li>
                                            <a href="{{ path('test33_edit', { 'id': test33.id }) }}">edit</a>
                                        </li>
                                    </ul>
                                </td>
                            </tr>
                        {% endfor %}
                            </tfoot>

                    </table>
                </div>
            </div>
    </section>
    <ul>
        <li>
                    </li>
    </ul>
{% endblock %}
